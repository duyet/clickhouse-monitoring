{
  "name": "clickhouse-monitor",
  "description": "Simple ClickHouse UI that relies on system tables to help monitor and provide overview of your cluster",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@10.18.2",
  "repository": {
    "url": "https://github.com/duyet/clickhouse-monitoring"
  },
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "build": "bun --bun next build --webpack",
    "start": "bun --bun next start",
    "start:coverage": "nyc --silent node server",
    "cf:build": "bun run build && opennextjs-cloudflare build --skipBuild",
    "cf:preview": "bun run cf:build && opennextjs-cloudflare preview",
    "cf:config": "bun scripts/set-secrets.ts",
    "cf:deploy": "bun run cf:config && bun run cf:build && wrangler deploy",
    "cf:typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "cy:open": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "fmt": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "bun jest",
    "jest": "jest --coverage",
    "test-queries-config": "jest --coverage --testPathPattern=query-config --testPathIgnorePatterns=/node_modules/",
    "preinstall": "npx only-allow bun"
  },
  "dependencies": {
    "@clickhouse/client": "^0.3.0",
    "@clickhouse/client-web": "^0.3.1",
    "@headlessui/tailwindcss": "^0.2.0",
    "@hookform/resolvers": "^5.0.0",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.10",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-table": "^8.20.5",
    "@uiw/react-heat-map": "^2.2.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/functions": "^3.0.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-6fc168f-20241025",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dayjs": "^1.11.10",
    "dedent": "^1.5.3",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.562.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.53.1",
    "react-markdown": "^10.0.0",
    "react-resizable-panels": "^3.0.0",
    "recharts": "^2.15.3",
    "server-only-context": "^0.1.0",
    "sonner": "^2.0.3",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "usehooks-ts": "^3.1.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@codecov/webpack-plugin": "^1.0.0",
    "@cypress/code-coverage": "^3.14.0",
    "@opennextjs/cloudflare": "^1.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "cypress": "^15.0.0",
    "cypress-real-events": "^1.13.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "postcss": "^8",
    "react-is": "^19.1.0",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.2.5",
    "turbo": "^2.0.6",
    "typescript": "^5",
    "wrangler": "^4.40.3"
  },
  "bun": {
    "onlyBuiltDependencies": [
      "cypress",
      "sharp"
    ]
  }
}
