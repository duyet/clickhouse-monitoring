    "miniflare": "^4.20260205.0",
    "next": "^15.5.12",
    "next-themes": "^0.4.6",
    "qs": "^6.14.1",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4.6.2",
    "recharts": "2.15.4",
    "server-only-context": "^0.1.0",
    "sonner": "^2.0.7",
    "sql-formatter": "^15.7.0",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tar": "^7.5.7",
    "test-exclude": "^7.0.1",
    "tw-animate-css": "^1.4.0",
    "undici": "^7.21.0",
    "usehooks-ts": "^3.1.1",
    "wait-on": "^9.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@codecov/webpack-plugin": "^1.9.1",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@cypress/code-coverage": "^3.14.7",
    "@opennextjs/cloudflare": "^1.16.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.2.2",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "cypress": "^15.10.0",
    "cypress-real-events": "^1.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "react-is": "^19.2.4",
    "shadcn": "^3.8.4",
    "start-server-and-test": "^2.1.3",
    "ts-jest": "^29.4.6",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3",
    "wrangler": "^4.63.0"
  },
  "bun": {
    "onlyBuiltDependencies": [
      "cypress",
      "sharp"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "*.{json,jsonc}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  }
}
