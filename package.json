{
  "name": "clickhouse-monitor",
  "description": "Simple ClickHouse UI that relies on system tables to help monitor and provide overview of your cluster",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "repository": {
    "url": "https://github.com/duyet/clickhouse-monitoring"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "start:coverage": "nyc --silent node server",
    "cf:build": "opennextjs-cloudflare build",
    "cf:preview": "pnpm run cf:build && opennextjs-cloudflare preview",
    "cf:deploy": "pnpm run cf:build && opennextjs-cloudflare deploy",
    "cf:upload": "pnpm run cf:build && opennextjs-cloudflare upload",
    "cf:typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "next lint",
    "cy:open": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "fmt": "prettier --write \"**/*.{ts,tsx,md,json}\" --cache",
    "test": "pnpm jest",
    "jest": "jest --coverage",
    "test-queries-config": "jest --coverage --testPathPattern=query-config",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@clickhouse/client": "^0.3.0",
    "@clickhouse/client-web": "^0.3.1",
    "@headlessui/tailwindcss": "^0.2.0",
    "@hookform/resolvers": "^5.0.0",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-collapsible": "^1.1.8",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.10",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-table": "^8.20.5",
    "@tremor/react": "^3.18.7",
    "@uiw/react-heat-map": "^2.2.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/functions": "^2.0.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-6fc168f-20241025",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dayjs": "^1.11.10",
    "dedent": "^1.5.3",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.544.0",
    "next": "^15.3.1",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.53.1",
    "react-markdown": "^10.0.0",
    "react-resizable-panels": "^3.0.0",
    "recharts": "^2.15.3",
    "server-only-context": "^0.1.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "usehooks-ts": "^3.1.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@codecov/webpack-plugin": "^1.0.0",
    "@cypress/code-coverage": "^3.14.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@next/eslint-plugin-next": "^15.0.0",
    "@opennextjs/cloudflare": "^1.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "^14.3.2",
    "cypress-real-events": "^1.13.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-cypress": "^5.0.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-is": "^19.1.0",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.2.5",
    "turbo": "^2.0.6",
    "typescript": "^5",
    "wrangler": "^4.40.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "cypress",
      "sharp"
    ]
  }
}
