{
  "name": "clickhouse-monitor",
  "description": "Simple ClickHouse UI that relies on system tables to help monitor and provide overview of your cluster",
  "version": "0.2.0",
  "private": true,
  "packageManager": "bun@1.3.3",
  "repository": {
    "url": "https://github.com/duyet/clickhouse-monitoring"
  },
  "scripts": {
    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "start:coverage": "nyc --silent node server",
    "cf:build": "bun run build && opennextjs-cloudflare build --skipBuild",
    "cf:preview": "bun run cf:build && opennextjs-cloudflare preview",
    "cf:config": "bun scripts/set-secrets.ts",
    "cf:deploy": "bun run cf:config && bun run cf:build && wrangler deploy",
    "cf:typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "cy:open": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "fmt": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "bun test",
    "test:unit": "bun test lib/__tests__ app/overview/__tests__ components/**/__tests__",
    "test:query-config": "bun test lib/query-config",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "preinstall": "npx only-allow bun",
    "prepare": "husky"
  },
  "dependencies": {
    "@actions/github": "^9.0.0",
    "@aws-sdk/client-cloudfront": "^3.985.0",
    "@aws-sdk/client-sqs": "^3.985.0",
    "@aws-sdk/core": "^3.973.7",
    "@aws-sdk/credential-provider-web-identity": "^3.972.5",
    "@aws-sdk/nested-clients": "^3.985.0",
    "@aws-sdk/xml-builder": "^3.972.4",
    "@clickhouse/client": "^0.3.1",
    "@clickhouse/client-web": "^0.3.1",
    "@cypress/request": "^3.0.10",
    "@cypress/webpack-preprocessor": "^7.0.2",
    "@dagrejs/dagre": "^1.1.8",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/tailwindcss": "^0.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@isaacs/brace-expansion": "^5.0.1",
    "@istanbuljs/load-nyc-config": "^1.1.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@opennextjs/aws": "^3.9.15",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
